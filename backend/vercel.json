{
    "builds" : [
        {
            "src": "app/main.py",
            "use": "@vercel/python"
        },
        {
          "src": "alembic.ini",
          "use": "@vercel/static-build"
        }
    ],
    "routes" : [
        {
            "src" : "/(.*)",
            "dest" : "app/main.py"
        }
    ],
    "buildCommand": "pip install -r requirements.txt && alembic upgrade head"

}